{
    "name": "audetv/control-desk",
    "description": "Control Desk App",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
      "process-timeout": 0,
      "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "symfony/console": "^6.2"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "require-dev": {
      "roave/security-advisories": "dev-master"
    },
    "scripts": {
      "lint": "find bin config public src -name \"*.php\" -print0 | xargs -0 -n1 php -l",
      "app": "php bin/app.php --ansi"
    },
    "authors": [
        {
            "name": "Aleksey Gusev"
        }
    ]
}
