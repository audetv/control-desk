{
    "name": "audetv/control-desk",
    "description": "Control Desk App",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
      "process-timeout": 0,
      "sort-packages": true
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "php-di/php-di": "^7.0",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "symfony/console": "^6.2"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "require-dev": {
        "overtrue/phplint": "^5.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.6"
    },
    "scripts": {
      "lint": "phplint",
      "cs-check": "phpcs",
      "cs-fix": "phpcbf",
      "psalm": "psalm",
      "app": "php bin/app.php --ansi"
    },
    "authors": [
        {
            "name": "Aleksey Gusev"
        }
    ]
}
